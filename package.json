{
  "private": true,
  "name": "feedback-widget",
  "packageManager": "pnpm@9.15.1",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "dotenv -e .env -- turbo run dev",
    "build": "dotenv -e .env -- turbo run build",
    "db:generate": "dotenv -e .env -- turbo run db:generate",
    "db:migrate:dev": "dotenv -e .env -- turbo run db:migrate:dev",
    "db:migrate:deploy": "dotenv -e .env -- turbo run db:migrate:deploy",
    "db:migrate:reset": "dotenv -e .env -- turbo run db:migrate:reset",
    "db:push": "dotenv -e .env -- turbo run db:push",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run test"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "7.2.0"
  },
  "devDependencies": {
    "dotenv-cli": "^11.0.0",
    "prettier": "^3.2.5",
    "prisma": "7.2.0",
    "tsx": "4.19.1",
    "turbo": "^2.7.4"
  }
}